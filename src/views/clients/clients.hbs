<div class="container">
    <div class="card">
        <div class="card-body">
                {{#each clients}}
            <div class="row col-12">
                    <div class="form-group">
                        <h5><strong>nome: </strong>{{dataValues.name}}</h5>
                        <h5><strong>telefone: </strong> {{dataValues.telephone}}</h5>
                        <a href="{{dataValues.photograph}}"><strong>Foto: </strong> {{dataValues.photograph}}</a>
                        <p><strong>stage: </strong>{{dataValues.stage}}</p>
                    </div>
                    <hr>
            </div>
                {{/each}}
        </div>
    </div>
</div>